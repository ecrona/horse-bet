{
  "name": "horse-bet",
  "version": "0.3.1",
  "description": "A small betting game specified for soccer tournaments",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register -P backend/tsconfig.json backend/src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "webpack-dev-server",
    "build": "webpack --env.production",
    "start:dev": "nodemon",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "format": "prettier --write \"**/*.ts\"",
    "build:client:prod": "webpack --env.production",
    "build:server:prod": "rimraf dist/server/* && tsc -P src/server/tsconfig.json",
    "start:client:dev": "webpack-dev-server --config webpack.config.ts",
    "start:client:dev:withMock": "cross-env USE_MOCK=true webpack-dev-server --config webpack.config.ts",
    "start:server:dev": "nodemon",
    "start:server:prod": "node dist/server/src/server/main.js",
    "migration:prepare": "ts-node scripts/prepare-migrations.ts",
    "migration:generate": "npm run migration:prepare && ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "npm run migration:prepare && ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "npm run migration:prepare && ts-node ./node_modules/typeorm/cli.js migration:revert",
    "deploy": "npm install && ts-node scripts/configure.ts && npm run build:client:prod && npm run migration:run",
    "firebase": "firebase"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecrona/horse-bet.git"
  },
  "author": "Eddie Crona & Viktor Carl√©n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecrona/horse-bet/issues"
  },
  "homepage": "https://github.com/ecrona/horse-bet#readme",
