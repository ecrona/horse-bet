{
  "name": "horse-bet",
  "version": "5.0.0",
  "description": "A small betting game",
  "scripts": {
    "vite-dev": "vite",
    "vite-build": "tsc && vite build",
    "vite-server": "vite preview",
    "type-check": "tsc",
    "start": "ts-node -r tsconfig-paths/register -P backend/tsconfig.json backend/src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "vite",
    "build": "tsc && vite build",
    "start:dev": "nodemon",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "format": "prettier --write \"**/*.ts\"",
    "build:client:prod": "tsc && vite build",
    "build:server:prod": "rimraf dist/server/* && tsc -P server/tsconfig.json",
    "start:client:dev": "vite",
    "start:client:dev:withMock": "cross-env USE_MOCK=true vite",
    "start:server:dev": "nodemon",
    "start:server:prod": "node dist/server/server/main.js",
    "command:sync": "ts-node -P server/tsconfig.json -r tsconfig-paths/register server/cli.ts sync:fixtures 3",
    "command:mail:invite": "ts-node -P server/tsconfig.json -r tsconfig-paths/register server/cli.ts mail:invite",
    "command:mail:notify": "ts-node -P server/tsconfig.json -r tsconfig-paths/register server/cli.ts mail:notify",
    "migration:prepare": "ts-node scripts/prepare-migrations.ts",
    "migration:generate": "npm run migration:prepare && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "npm run migration:prepare && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "npm run migration:prepare && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "deploy": "npm install && ts-node scripts/configure.ts && npm run build:client:prod && npm run migration:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecrona/horse-bet.git"
  },
  "author": "Eddie Crona & Viktor Carl√©n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecrona/horse-bet/issues"
  },
  "homepage": "https://github.com/ecrona/horse-bet#readme",
  "dependencies": {
    "@material-ui/core": "4.9.1",
    "@material-ui/icons": "4.9.1",
    "@nest-modules/mailer": "1.1.3",
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "6.11.5",
    "@nestjs/passport": "1.1.0",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/typeorm": "6.2.0",
    "@types/history": "4.7.5",
    "@types/jwt-client": "0.2.2",
    "@types/node": "12.7.11",
    "@types/passport-google-oauth": "1.0.39",
    "@types/react-router": "5.1.4",
    "@types/react-router-dom": "5.1.3",
    "@types/redux-logger": "3.0.7",
    "@types/validatorjs": "3.7.1",
    "bcrypt": "5.0.1",
    "clsx": "1.0.4",
    "cookie-parser": "1.4.3",
    "date-fns": "1.29.0",
    "debounce": "1.2.0",
    "es6-collections": "0.5.6",
    "es6-promise": "4.2.4",
    "history": "4.10.1",
    "iso-3166-2": "1.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "8.3.0",
    "jwt-client": "0.2.1",
    "mysql": "2.16.0",
    "nestjs-command": "^3.1.0",
    "passport": "0.4.0",
    "passport-google-oauth": "1.0.0",
    "passport-jwt": "4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "7.2.5",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reflect-metadata": "0.1.13",
    "reselect": "4.0.0",
    "rxjs": "6.2.2",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.4.2",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/reselect": "2.2.0",
    "@vitejs/plugin-react": "^1.1.3",
    "autoprefixer": "^10.4.0",
    "cors": "2.8.5",
    "cross-env": "7.0.0",
    "kill-port": "1.6.1",
    "nodemon": "2.0.2",
    "postcss": "^8.4.5",
    "redux-logger": "3.0.6",
    "tailwindcss": "^3.0.7",
    "tslint": "5.9.1",
    "typescript": "^4.5.4",
    "vite": "2.6.x"
  }
}
